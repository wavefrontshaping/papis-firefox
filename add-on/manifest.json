{

  "description": "Add paper to papis from current url.",
  "manifest_version": 2,
  "name": "Papis",
  "version": "0.1.3",
  "icons": {
    "32": "icons/paper.svg",
    "48": "icons/paper.svg",
    "64": "icons/paper.svg",
    "96": "icons/paper.svg"
  },
  "applications": {
    "gecko": {
      "id": "papis_connector@papis",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  }	,

  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/paper.svg",
    "theme_icons": [
      {
        "light": "icons/dark-paper.svg",
        "dark": "icons/paper.svg",
        "size": 16
      },
      {
        "light": "icons/dark-paper.svg",
        "dark": "icons/paper.svg",
        "size": 32
      },
      {
        "light": "icons/dark-paper.svg",
        "dark": "icons/paper.svg",
        "size": 64
      },
      {
        "light": "icons/dark-paper.svg",
        "dark": "icons/paper.svg",
        "size": 96
      }
    ]
  },
  
  "permissions": [
    "nativeMessaging",
    "tabs"
    ]

}
